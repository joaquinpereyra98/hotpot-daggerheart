{
  "eslint.enable": true,
  "eslint.validate": ["javascript", "handlebars", "css", "markdown"],
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[css]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[handlebars]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  }
}
